#ayuda
#vectores con infop
edades<-c(32,35,43,25)
estcony<-c("casado","soltero", "soltero", "union libre")
sexo<-c(1,2,2,1)

View(sexo) #observas tabla
sexo[4] #posicion 4
sexo[c(1,3)] #posicion 1, 3

#bases de datos
base1<-data.frame(edades,estcony,sexo)
View(base1) #ver base de datos
table(base1$sexo) #cuenta cuantos hay de hombres o mujeres (frecuencia)
